# Application
SPRING_PROFILES_ACTIVE=local
TAG=latest

# Database
POSTGRES_USER=dpms
POSTGRES_PASSWORD=change_this_in_production
POSTGRES_DB=dpms
POSTGRES_HOST=postgres

# Redis
REDIS_PASSWORD=change_this_in_production
SPRING_REDIS_HOST=redis

# MinIO
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=change_this_in_production
MINIO_ENDPOINT=http://minio:9000

# JWT
JWT_SECRET=change_this_to_a_random_secure_key_in_production

# Mail
MAIL_USERNAME=your-email@gmail.com
MAIL_PASSWORD=your-app-specific-password

# Monitoring
GRAFANA_ADMIN_PASSWORD=change_this_in_production

# ELK Stack
ELASTIC_PASSWORD=change_this_in_production       # Used by Elasticsearch, Logstash, and Kibana
KIBANA_PASSWORD=change_this_in_production       # Used for Kibana system user
LOGSTASH_PASSWORD=change_this_in_production     # Used by Logstash to connect to Elasticsearch

# Coolify Deployment
COOLIFY_API_URL=your-coolify-url
COOLIFY_API_TOKEN=your-coolify-token
COOLIFY_SERVICE_ID=your-service-id

# Docker Hub
DOCKERHUB_USERNAME=isresearch
DOCKERHUB_PASSWORD=your-dockerhub-password
